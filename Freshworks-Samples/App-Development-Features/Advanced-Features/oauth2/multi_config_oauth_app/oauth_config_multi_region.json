{
  "client_id": "<%= (oauth_iparams.region == 'US') ? 'xxxxx': (oauth_iparams.region == 'EUC') ? 'yyyyy' : 'zzzzz' %>",
  "client_secret": "<%= (oauth_iparams.region == 'US') ? 'xxxxx': (oauth_iparams.region == 'EUC') ? 'yyyyy' : 'zzzzz' %>",
  "authorize_url": "https://login.live.com/oauth20_authorize.srf",
  "token_url": "https://login.live.com/oauth20_token.srf",
  "options": {
    "scope": "onedrive.readwrite offline_access"
  },
  "token_type": "account",
  "oauth_iparams": {
    "region": {
      "display_name": "Region",
      "description": "Please enter the region - US/EUC",
      "type": "text",
      "required": false
    }
  }
}